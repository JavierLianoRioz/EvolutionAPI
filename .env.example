# Configuración de autenticación básica de n8n
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=password

# Configuración de zona horaria
TZ=America/Bogota
GENERIC_TIMEZONE=America/Bogota

# Configuración de Evolution API
API_KEY=tu_api_key_secreta
SERVER_URL=http://localhost:8080
DATABASE_ENABLED=false

# Configuración del flujo de trabajo n8n
IA_API_URL=URL_DE_LA_API_DE_IA
WHATSAPP_DESTINATION_NUMBER=NUMERO_DE_WHATSAPP_DESTINO

# Configuración del Dockerfile
MAINTAINER_EMAIL=tu_nombre@example.com